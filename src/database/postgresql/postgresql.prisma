generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  role      String[]
  status    Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
